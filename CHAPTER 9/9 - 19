<!DOCTYPE html>
<html>
    <head>
        <title>9 - 19 예제</title>
        <meta charset="UTF-8">
    </head>
    <body>
        <script type="text/javascript">
            
            var numarr = [];
            var max = 0;
            var min = 0;
  
            for(i=0;i<3;i++){
                numarr[i] = parseInt(prompt((i+1)+'번째 숫자를 입력하세요'));
            }
  
            for(i=0;i<3;i++){
  	            if(numarr[i]>max){
    	        max = numarr[i]
                }
            }
  
            min = numarr[0];
            for(i=0;i<3;i++){
  	            if(numarr[i]<min){
    	        min = numarr[i]
                }
            }
  
            document.write('최대값은'+max+'이다.<br>');
            document.write('최소값은'+min+'이다.');
        </script>
    </body>
</html>
